
<div>

	@if (IsShowingNodeSelection)
	{
		<div @onclick="@(() => IsShowingNodeSelection = false)" style="background-color: var(--mud-palette-overlay-dark); width: 100%; height: 100%; position: absolute; z-index: 1">
			<NodeDev.Blazor.Components.NodeSelectionPopup.NodeSelection PositionX="NodeSelectionX" PositionY="NodeSelectionY" GraphCanvas="this" OnNodeTypeSelected="OnNewNodeTypeSelected" />
		</div>
	}

	<div @key="Id" id="@Id" style="width:3000px; height:3000px;"></div>

</div>